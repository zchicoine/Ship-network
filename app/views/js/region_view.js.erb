var temp_region = '<%= escape_javascript( @saved_name )%>'

// make sure the map is in place
if($("#googleMap").length == 0){

    $("#outer-map").html(" <%= escape_javascript(render 'google_map/index')%>");
    var spec = {'new_map':true,'layer':REGION_LEVEL, 'region_name':temp_region };
    reload_the_map_with_specification(spec);
    MainViewGeneratorInstance.regionView(temp_region);

}else{

    MainViewGeneratorInstance.regionView(temp_region);
}


