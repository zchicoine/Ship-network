// from the main banner log in button
<% if error.nil? %>
$('.banner1').html("<%= escape_javascript( render 'main_pages/login', error: nil )%>")

<% else %>
$('#broker_username').parent().addClass("has-error has-feedback")
$('#broker_username').parent().append("<span class='glyphicon glyphicon-remove form-control-feedback'></span>")
$('#broker_password').parent().addClass("has-error has-feedback")
$('#broker_password').parent().append("<span class='glyphicon glyphicon-remove form-control-feedback'></span>")
<% end %>